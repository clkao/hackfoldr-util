    "unist-builder": "^1.0.2",
    "unist-util-select": "^1.5.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
